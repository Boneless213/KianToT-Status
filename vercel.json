{
    "version": 2,
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.7.2"
        }
    },
    "rewrites": [
        {
            "source": "/api/debug_files",
            "destination": "/api/debug_files.php"
        },
        {
            "source": "/build/:path*",
            "destination": "/public/build/:path*"
        },
        {
            "source": "/Vids/:path*",
            "destination": "/public/Vids/:path*"
        },
        {
            "source": "/PICS/:path*",
            "destination": "/public/PICS/:path*"
        },
        {
            "source": "/GIF/:path*",
            "destination": "/public/GIF/:path*"
        },
        {
            "source": "/favicon.ico",
            "destination": "/public/favicon.ico"
        },
        {
            "source": "/robots.txt",
            "destination": "/public/robots.txt"
        },
        {
            "source": "/test_static.txt",
            "destination": "/public/test_static.txt"
        },
        {
            "source": "/(.*)",
            "destination": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "APP_KEY": "base64:wo0zXWeD+2pnFpum+evdj/HBYn7Fib6BYfMWPmt1khM=",
        "LOG_CHANNEL": "stderr"
    }
}