{
    "version": 2,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.1"
        }
    },
    "rewrites": [
        {
            "source": "/build/(.*)",
            "destination": "/public/build/$1"
        },
        {
            "source": "/Vids/(.*)",
            "destination": "/public/Vids/$1"
        },
        {
            "source": "/PICS/(.*)",
            "destination": "/public/PICS/$1"
        },
        {
            "source": "/GIF/(.*)",
            "destination": "/public/GIF/$1"
        },
        {
            "source": "/favicon.ico",
            "destination": "/public/favicon.ico"
        },
        {
            "source": "/robots.txt",
            "destination": "/public/robots.txt"
        },
        {
            "source": "/(.*)",
            "destination": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "APP_KEY": "base64:wo0zXWeD+2pnFpum+evdj/HBYn7Fib6BYfMWPmt1khM=",
        "APP_URL": "https://kiantot.vercel.app",
        "ASSET_URL": "/",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp",
        "CACHE_STORE": "array",
        "SESSION_DRIVER": "cookie",
        "LOG_CHANNEL": "stderr"
    }
}